" execute pathogen#infect()
syntax on 
filetype plugin indent on

colorscheme codedark

set number "Line Numbers
set cursorline
set autoindent
set incsearch "search as chars entered
set hlsearch "highlight matches
set tabstop=2
set softtabstop=2
set shiftwidth=2
set expandtab

let g:airline_powerline_fonts=1
